<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:scg="http://www.adobe.com/social/scg/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:designPath="/etc/designs/social/console"
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:lastReplicated="{Date}2016-10-23T11:44:05.052-07:00"
        cq:lastReplicatedBy="msm-service"
        cq:lastReplicationAction="Activate"
        cq:template="/libs/social/console/templates/base_page"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Blog"
        jcr:uuid="db61c4cf-5779-4490-90d4-9513e71d24ce"
        sling:resourceType="social/console/components/basepage"
        etcpath="/etc/community/templates/functions/reference/blog"
        navTitle="Blog"
        pageTitle="Blog">
        <clientlibslist
            jcr:primaryType="nt:unstructured"
            scg:requiredClientLibs="[cq.ckeditor,cq.social.hbs.voting,cq.social.hbs.journal,cq.social.hbs.signup,cq.social.hbs.breadcrumbs,personalization.core.kernel,personalization.clientcontext.kernel,personalization.stores.kernel,personalization.kernel,cq.social.hbs.journal_sidebar]"
            sling:resourceType="social/console/components/clientlibslist"/>
        <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="social/console/components/hbs/rightcolumncontent">
            <breadcrumbs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="social/console/components/hbs/breadcrumbs"/>
            <primary
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <blog
                    cq:privilegedMembers="[]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="social/journal/components/hbs/journal"
                    acceptFileTypes="[.jpeg\, .jpg\, .gif\, .png\, .pdf\, .ppt\, .pptx\, .doc\, .docx]"
                    allowCloseComments="true"
                    allowDeleteComments="true"
                    allowDenyComments="true"
                    allowFileUploads="{Boolean}true"
                    allowFlagComments="true"
                    allowFollowing="true"
                    allowPrivilegedMembers="{Boolean}false"
                    allowRepliesToComments="{Boolean}true"
                    allowSubscriptions="true"
                    allowTagging="true"
                    allowVoting="true"
                    feedEnabled="true"
                    flagModerateThreshold="1"
                    flagViewThreshold="5"
                    maxFileSize="104857600"
                    maxImageFileSize="2097152"
                    maxMessageLength="104857600"
                    maxSubjectLength="104857600"
                    richTextEditor="true"
                    rteEnabled="true"
                    useFlagReasonFreeformText="false"
                    useFlagReasonList="false">
                    <userlist
                        cq:lastRolledout="{Date}2015-09-09T01:30:19.652-07:00"
                        cq:lastRolledoutBy="admin"
                        jcr:created="{Date}2015-03-04T16:10:55.434-05:00"
                        jcr:createdBy="admin"
                        jcr:lastModified="{Date}2015-09-09T01:30:19.650-07:00"
                        jcr:lastModifiedBy="admin"
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="social/messaging/components/hbs/userchooser"
                        searchDisplayName="true"
                        searchEmail="true"
                        searchGivenName="true"
                        searchLastName="true"
                        searchMiddleName="true"
                        showUsers="true"/>
                </blog>
            </primary>
            <auxilary
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/foundation/components/responsivegrid">
                <authorssidebar
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="social/journal/components/hbs/sidebar"
                    viewType="authors"/>
                <categoriessidebar
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="social/journal/components/hbs/sidebar"
                    viewType="categories"/>
                <archivessidebar
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="social/journal/components/hbs/sidebar"
                    viewType="archives"/>
            </auxilary>
        </content>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Decimal}768"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Decimal}992"/>
                <mediumscreen
                    jcr:primaryType="nt:unstructured"
                    title="Medium Screen"
                    width="{Decimal}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
